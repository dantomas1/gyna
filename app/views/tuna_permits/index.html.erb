<%= will_paginate @tuna_permits %>

<% @tuna_permits.each do |permit| %>
<div class="row">
 <div class = "col-md-4 col-md-offset-4 center">
  <h5>Vesel: <%= permit.vessel %></h5>
  <h5>Contact: <%= permit.contact %></h5>
  <h5><%= permit.address %></h5>
  <h5><%= permit.cityz %></h5>
  <h5><%= permit.state %>, USA</h5>
  <h5>Phone: <%= permit.fax %></h5>
  <h5>Fax: <%= permit.fax %></h5>
 <hr />
 <br/>
 <br/>
 </div>
</div>
<% end %>

<%= will_paginate @tuna_permits %>

